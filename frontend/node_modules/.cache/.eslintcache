oading(false);\n\n        } catch (err) {\n            setError(err.message);\n            setLoading(false);\n        }\n    };\n\n    fetchData();\n\n\n},[url]);\n\n\n    return {\n        data,\n        error,\n        loading,\n    };\n};\n\nexport default useFetch;","C:\\Users\\joelg\\Downloads\\MernStack-Tour-Management-main\\MernStack-Tour-Management-main\\tour-management\\frontend\\src\\admin\\AdminNav.jsx",["210"],[],"C:\\Users\\joelg\\Downloads\\MernStack-Tour-Management-main\\MernStack-Tour-Management-main\\tour-management\\frontend\\src\\router\\ProtectedRoute.js",[],[],"C:\\Users\\joelg\\Downloads\\MernStack-Tour-Management-main\\MernStack-Tour-Management-main\\tour-management\\frontend\\src\\admin\\AllTours.jsx",["211","212","213"],[],"import React, { useState, useEffect } from 'react';\nimport { Container, Row, Col } from 'reactstrap';\nimport axios from 'axios';\nimport { toast } from 'react-toastify';\nimport '../styles/alltours.css';\n\nconst AllTours = () => {\n  const [tours, setTours] = useState([]);\n\n  useEffect(() => {\n    fetchTours();\n  }, []);\n\n  const fetchTours = async () => {\n    try {\n      const response = await axios.get('http://localhost:4000/tours/');\n      setTours(response.data.data);\n    } catch (error) {\n      console.error('Error fetching tours:', error);\n    }\n  };\n\n  const handleDeleteTour = async (tourId) => {\n    try {\n      // Send a DELETE request to remove the tour with the specified ID\n      await axios.delete(`http://localhost:4000/tours/${tourId}`);\n      // Update the tours list by removing the deleted tour\n      setTours((prevTours) => prevTours.filter((tour) => tour._id !== tourId));\n    } catch (error) {\n      console.error('Error deleting tour:', error);\n    }\n  };\n\n\n  return (\n    <Container className=\"all-tours-container\">\n      <h2>All Tours</h2>\n      {tours.map((tour) => (\n        <div key={tour._id} className=\"tour-card\">\n          <img src={tour.photo} alt={tour.title} c