urs((prevTours) => prevTours.filter((tour) => tour._id !== tourId));\n    } catch (error) {\n      console.error('Error deleting tour:', error);\n    }\n  };\n  \n \n  return (\n    \n    \n    <div className=\"dashboard-container\">\n      <h1 className=\"dashboard-title\">Dashboard</h1>\n      <nav className=\"dashboard-nav\">\n        {/* Wrap \"User\" and \"Users\" links in a div */}\n        <div className=\"nav-box\">\n          <Link to=\"/dashboard/all-tours\">All Tours</Link>\n          <Link to=\"/dashboard/users\">Users</Link>\n        </div>\n      </nav>\n      <div className=\"table-container\"> {/* Wrap the table in a container */}\n        <h2>Tour Packages</h2>\n        <table className=\"tour-table\"> {/* Apply the tour-table class here */}\n          <thead>\n            <tr>\n            <th>Photo</th>\n              <th>Title</th>\n              <th>Price</th>\n              <th>Description</th>\n              <th>City</th>\n              <th>Address</th>\n             \n              <th>Actions</th> {/* Add a new column for actions */}\n            </tr>\n          </thead>\n          <tbody>\n            {tours.map((tour) => (\n              <tr key={tour._id}>\n                 <td>\n                  <img src={tour.photo} alt={`Image for ${tour.title}`} width=\"100\" height=\"100\" />\n                </td>\n                <td>{tour.title}</td>\n                <td>${tour.price}</td>\n                <td>{tour.desc}</td>\n                <td>{tour.city}</td>\n                <td>{tour.address}</td>\n               \n                <td>\n                  <button onClick={() => handleDeleteTour(tour._id)}>Delete</button>\n                </td>\n              </tr>\n            ))}\n          </tbody>\n        </table>\n      </div>\n    </div>\n  );\n};\n\nexport default Dashboard;","C:\\Users\\joelg\\Downloads\\MernStack-Tour-Management-main\\MernStack-Tour-Management-main\\tour-management\\frontend\\src\\admin\\Users.jsx",[],[],"C:\\User